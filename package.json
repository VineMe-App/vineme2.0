{
  "name": "vineme-mobile-app",
  "version": "1.0.0",
  "main": "expo-router/entry",
  "scripts": {
    "start": "expo start",
    "start:expo-go": "USE_EXPO_GO=true expo start --clear",
    "start:dev-client": "expo start --dev-client --clear",
    "android": "expo start --dev-client",
    "android:expo-go": "USE_EXPO_GO=true expo start --clear",
    "android:simulator": "expo start --dev-client --android",
    "ios": "expo start --dev-client",
    "ios:expo-go": "USE_EXPO_GO=true expo start --clear",
    "ios:simulator": "expo start --dev-client --ios",
    "web": "expo start --web",
    "firewall:enable": "./scripts/expo-firewall-setup.sh enable",
    "firewall:disable": "./scripts/expo-firewall-setup.sh disable",
    "firewall:status": "./scripts/expo-firewall-setup.sh status",
    "network:debug": "./scripts/network-troubleshoot.sh",
    "lint": "eslint . --ext .ts,.tsx",
    "lint:fix": "eslint . --ext .ts,.tsx --fix",
    "format": "prettier --write .",
    "format:check": "prettier --check .",
    "test": "jest --verbose",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "test:ci": "jest --coverage --watchAll=false --passWithNoTests",
    "tests:archive": "node scripts/archive-tests.js",
    "test:unit": "jest --testPathPatterns='src/(services|hooks|utils|components|stores|providers).*\\.test\\.(ts|tsx)$'",
    "test:integration": "jest --testPathPatterns='src/__tests__/integration.*\\.test\\.(ts|tsx)$'",
    "test:e2e": "jest --testPathPatterns='src/__tests__/e2e.*\\.test\\.(ts|tsx)$'",
    "test:styling-system": "node src/__tests__/styling-system/test-runner.js all",
    "test:styling-system:integration": "node src/__tests__/styling-system/test-runner.js integration",
    "test:styling-system:visual": "node src/__tests__/styling-system/test-runner.js visual-regression",
    "test:styling-system:accessibility": "node src/__tests__/styling-system/test-runner.js accessibility",
    "test:styling-system:performance": "node src/__tests__/styling-system/test-runner.js performance",
    "test:styling-system:e2e": "node src/__tests__/styling-system/test-runner.js e2e",
    "test:ios": "expo start --ios --clear",
    "test:android": "expo start --android --clear",
    "build:ios": "expo build:ios",
    "build": "eas build --profile development --platform android",
    "preview:ios": "expo install --ios && expo start --ios",
    "preview:android": "expo install --android && expo start --android"
  },
  "dependencies": {
    "@expo/metro-runtime": "~6.1.2",
    "@expo/vector-icons": "^15.0.2",
    "@react-native-async-storage/async-storage": "2.2.0",
    "@react-native-community/datetimepicker": "8.4.4",
    "@react-native-community/netinfo": "^11.4.1",
    "@supabase/supabase-js": "^2.54.0",
    "@tanstack/react-query": "^5.84.2",
    "babel-plugin-module-resolver": "^5.0.2",
    "expo": "~54.0.0",
    "expo-blur": "~15.0.7",
    "expo-clipboard": "~8.0.7",
    "expo-constants": "~18.0.9",
    "expo-dev-client": "~6.0.14",
    "expo-device": "~8.0.9",
    "expo-file-system": "~19.0.17",
    "expo-font": "~14.0.9",
    "expo-image-picker": "~17.0.8",
    "expo-linking": "~8.0.8",
    "expo-location": "~19.0.7",
    "expo-notifications": "~0.32.12",
    "expo-router": "~6.0.11",
    "expo-secure-store": "~15.0.7",
    "pngjs": "^7.0.0",
    "react": "19.1.0",
    "react-dom": "19.1.0",
    "react-native": "0.81.4",
    "react-native-gesture-handler": "~2.28.0",
    "react-native-maps": "1.20.1",
    "react-native-safe-area-context": "~5.6.0",
    "react-native-screens": "~4.16.0",
    "react-native-web": "^0.21.0",
    "zustand": "^5.0.7"
  },
  "devDependencies": {
    "@babel/core": "^7.26.0",
    "@eslint/js": "^9.33.0",
    "@expo/config-plugins": "~54.0.1",
    "@expo/ngrok": "^4.1.3",
    "@expo/prebuild-config": "~54.0.0",
    "@tanstack/react-query-devtools": "^5.84.2",
    "@testing-library/jest-native": "^5.4.3",
    "@testing-library/react-native": "^13.2.2",
    "@types/react": "~19.1.10",
    "@typescript-eslint/eslint-plugin": "^8.39.0",
    "@typescript-eslint/parser": "^8.39.0",
    "eslint": "^8.57.1",
    "eslint-config-expo": "~10.0.0",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-prettier": "^5.5.4",
    "eslint-plugin-react": "^7.37.5",
    "eslint-plugin-react-hooks": "^5.2.0",
    "eslint-plugin-react-native": "^5.0.0",
    "husky": "^9.1.7",
    "jest": "~29.7.0",
    "jest-environment-jsdom": "^30.0.5",
    "lint-staged": "^16.2.4",
    "metro": "^0.83.1",
    "metro-config": "^0.83.1",
    "metro-resolver": "^0.83.1",
    "prettier": "^3.6.2",
    "react-test-renderer": "19.1.0",
    "typescript": "~5.9.2"
  },
  "private": true
}
